<?xml version="1.0" encoding="UTF-8"?>
<excel id="student" code="student" name="毕业学生成绩登记表">
    <colgroup>
        <col index="A" width='28em'></col>
        <col index="B" width='11em'></col>
        <col index="C" width='12em'></col>
        <col index="D" width='11em'></col>
        <col index="E" width='11em'></col>
        <col index="F" width='14em'></col>        
    </colgroup>
    <title>
        <tr height="16px" fontsize="18" fontweight="0" rowheight="35" center="1" >
            <td rowspan="1" colspan="6" value="广东药科大学成人高等教育" />
        </tr>
        <tr height="16px"  fontsize="22" fontweight="1" rowheight="45" center="1" >
            <td rowspan="1" colspan="6" value="毕业生成绩登记表" />
        </tr>
        <tr height="16px"  fontsize="14" fontweight="0" rowheight="25" center="0" >
            <td rowspan="1" colspan="3" value="学号：" />
            <td rowspan="4" colspan="6" value="姓名：" />
        </tr>
        <tr height="16px"  fontsize="12" fontweight="0" rowheight="25" center="0" >
            <td rowspan="1" colspan="1" value="专业：" />
            <td rowspan="2" colspan="3" value="学习形式：" />
            <td rowspan="4" colspan="5" value="培养层次：" />
            <td rowspan="6"  colspan="1" value="学制：" />
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="课程名称" />
            <th value="学时数" />
            <th value="考核方式" />
            <th value="学期" />
            <th value="成绩" />
            <th value="备注" />            
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="5">
            <td type="string" isnullable="false" maxlength="30" /><!--用户编号 -->
            <td type="string" isnullable="false" maxlength="50" /><!--姓名 -->
            <td type="numeric" format="##0" isnullable="false" /><!--年龄 -->
            <td type="enum" format="男,女" isnullable="true" /><!--性别 -->
            <td type="date" isnullable="false" maxlength="30" /><!--出生日期 -->
            <td type="enum" format="足球,篮球,乒乓球" isnullable="true" /><!--爱好 -->
        </tr>
    </tbody>
    <foot>
    	 <tr height="16px" fontsize="14" fontweight="0" rowheight="18" center="1" >
            <td rowspan="4" colspan="6"  />
        </tr>
        <tr height="16px"  fontsize="14" fontweight="0" rowheight="18" center="1" >
            <td rowspan="4" colspan="6"  />
        </tr>
    </foot>
</excel>